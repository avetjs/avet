{
  "lerna": "2.5.1",
  "version": "1.0.0-28",
  "changelog": {
    "repo": "avetjs/avet",
    "labels": {
      "PR: New Feature": ":rocket: New Feature",
      "PR: Breaking Change": ":boom: Breaking Change",
      "PR: Bug Fix": ":bug: Bug Fix",
      "PR: Enhancement": ":nail_care: Enhancement",
      "PR: Example": ":kissing_heart: Example",
      "PR: Docs": ":memo: Documentation"
    },
    "cacheDir": ".changelog"
  },
  "commands": {
    "build": {
      "scope": [
        "avet-shared",
        "avet-client",
        "avet-utils"
      ]
    }
  },
  "packages": [
    "packages/*",
    "bench/*",
    "examples/*"
  ],
  "npmClient": "npm",
  "npmClientArgs": [
    "--no-lockfile"
  ]
}
