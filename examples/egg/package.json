{
  "name": "avet-example-egg",
  "version": "1.0.0",
  "description": "this is a avet and egg example",
  "private": true,
  "scripts": {
    "start": "egg-scripts start --daemon",
    "stop": "egg-scripts stop",
    "dev": "npm run dev:egg & npm run dev:avet",
    "dev:egg": "egg-bin dev",
    "dev:avet": "avet-bin dev --rootDir ./web",
    "build": "avet-bin build --rootDir ./web"
  },
  "dependencies": {
    "avet": "^1.0.0-9",
    "avet-define": "^0.1.2",
    "avet-fetch": "^0.2.2",
    "avet-preact": "^0.2.3",
    "avet-proxy": "^1.0.0",
    "egg": "^2.0.0",
    "egg-avet": "^1.1.1",
    "egg-scripts": "^2.4.0"
  },
  "devDependencies": {
    "egg-bin": "^4.3.6",
    "avet-bin": "^1.0.0-9"
  },
  "tnpm": {
    "mode": "npm"
  },
  "engines": {
    "node": ">= 8"
  },
  "license": "MIT"
}
